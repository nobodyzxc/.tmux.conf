[global_config]
  dbus = False
  borderless = True
  extra_styling = False
  use_custom_url_handler = True
  putty_paste_style = True
[keybindings]
  full_screen = <Alt>Return
[profiles]
  [[default]]
    audible_bell = True
    background_color = "#1e1e1e"
    background_darkness = 0.8
    background_type = transparent
    cursor_color = "#edcf4f"
    font = DejaVu Sans Mono Bold 12
    foreground_color = "#f7f6ec"
    show_titlebar = False
    scrollbar_position = hidden
    scroll_on_keystroke = False
    palette = "#343935:#cf3f61:#7bb75b:#e9b32a:#4c9ad4:#a57fc4:#389aad:#fafaf6:#343935:#cf3f61:#7bb75b:#e9b32a:#4c9ad4:#a57fc4:#389aad:#fafaf6"
    login_shell = True
    custom_command = /bin/sh
    use_system_font = False
    bold_is_bright = True
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
