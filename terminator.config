[global_config]
  borderless = True
  dbus = False
  extra_styling = False
  putty_paste_style = True
[keybindings]
  full_screen = <Alt>Return
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    allow_bold = False
    audible_bell = True
    background_darkness = 0.6
    background_type = transparent
    cursor_color = "#aaaaaa"
    custom_command = /bin/sh
    font = DejaVu Sans Mono Bold 12
    foreground_color = "#ffffff"
    login_shell = True
    scroll_on_keystroke = False
    scrollbar_position = hidden
    show_titlebar = False
    use_system_font = False
